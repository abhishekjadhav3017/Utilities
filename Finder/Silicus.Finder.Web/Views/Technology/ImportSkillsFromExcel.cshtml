
<div style="align-items:center">
    <h2>Upload Skill Record Sheet</h2>
    @using (Html.BeginForm("ImportSkillsFromExcel", "Technology", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <label for="file">Upload File:</label>
        <input type="file" name="file" id="file" /><br><br>
        @ViewBag.Message;
        <br /><br />

        <div class="col-md-offset-6">
            <input type="submit" value="Upload File" class="btn btn-default" />
            <input type="button" value="Back To List" class="btn btn-default" onclick="@("window.location.href='" + @Url.Action("GetAllSkillSet", "Technology") + "'");" />
        </div>
    }
</div>


